{
  "name": "openframe-webapp",
  "private": true,
  "version": "0.1.6",
  "description": "A responsive front-end web app for Openframe.",
  "main": "",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --mode=development --config webpack.dev.js",
    "build": "./node_modules/.bin/webpack --mode=production --config webpack.prod.js",
    "start": "npm run dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "posttest": "npm run lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "dist": "npm run clean && webpack --mode=production --config webpack.dev.js",
    "lint": "eslint ./src",
    "copy": "copyfiles -f ./src/index.html ./src/hub.html ./src/favicon.ico ./dist",
    "clean": "rimraf dist/*",
    "release:major": "npm version major && git push upstream --follow-tags",
    "release:minor": "npm version minor && git push upstream --follow-tags",
    "release:patch": "npm run dist && git add . && git commit -m \"release-build.`date +%Y%m%d%H%m%S`\" && npm version patch && git push upstream --follow-tags",
    "deploy:dev": "npm run dist && git add . && git commit -m \"dev-build.`date +%Y%m%d%H%m%S`\" && git push origin dist && scp -r ./dist of-dev:/var/www",
    "deploy": "scp -r ./dist/* of:/var/www/openframe-webapp"
  },
  "repository": "https://github.com/OpenframeProject/Openframe-WebApp.git",
  "keywords": [
    "openframe",
    "openframe-webapp"
  ],
  "author": "Jonathan Wohl <jon@jonwohl.com>",
  "license": "AGPL-3.0+",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "bower-webpack-plugin": "^0.1.9",
    "chai": "^3.2.0",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^0.2.1",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^4.0.0",
    "file-loader": "^5.0.2",
    "glob": "^7.0.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "isparta-instrumenter-loader": "^1.0.0",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mini-css-extract-plugin": "^0.8.0",
    "minimist": "^1.2.0",
    "mocha": "^2.2.5",
    "node-sass": "^4.13.0",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.0.0",
    "react-addons-test-utils": "^15.0.0",
    "redux-devtools": "^3.5.0",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^8.0.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^1.0.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "babel-eslint": "^10.0.3",
    "bootstrap-sass": "^3.3.6",
    "compass-mixins": "jmwohl/compass-mixins",
    "core-js": "^2.0.0",
    "cross-storage": "^1.0.0",
    "dotenv-webpack": "^1.7.0",
    "faye": "^1.1.2",
    "glslCanvas": "^0.1.7",
    "i": "^0.3.6",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.14.1",
    "normalize.css": "^4.0.0",
    "normalizr": "^3.4.1",
    "react": "^16.12.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-masonry-component": "^6.2.1",
    "react-modal": "^3.11.1",
    "react-onclickoutside": "^6.9.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-scroll": "^0.4.4",
    "react-select": "^3.0.8",
    "react-spin": "^0.6.2",
    "react-transition-group": "^4.3.0",
    "react-waypoint": "^9.0.2",
    "redux": "^4.0.4",
    "redux-form": "^8.2.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  }
}
